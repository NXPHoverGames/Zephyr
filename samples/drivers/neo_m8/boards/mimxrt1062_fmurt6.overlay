/ {
  neom8: neom8 {
    status = "okay";
    compatible = "u-blox,neom8";
  };

  chosen {
    zephyr,gps1 = &lpuart2;
  };
};

&lpuart2 {
	status = "okay";
	current-speed = <115200>;
	pinctrl-0 = <&pinmux_lpuart2>;
	pinctrl-1 = <&pinmux_lpuart2_sleep>;
	pinctrl-names = "default", "sleep";
};
